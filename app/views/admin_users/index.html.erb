<div id="con">
	
<div class="container-fluid p-3 bg-dark text-white">
	<h1 align="center">My Test Application</h1>
	<%= link_to("<Home>",:controller=>'access',:action=>'menu')%>
		<div class="container admin_color" align="right">Logged in as :<%=session[:username]%>  </div>

</div>
<div>
	<%= link_to("create_new_user",:controller=>'admin_users',:action=>'new')%>
</div>
<div class="container">
	<table class="table table-hover">
		<thead class="tb_header">
		  <tr>
		  		<th>#</th>
		  		<th>First Name</th>
		  		<th>Last Name</th>
		  		<th>User Name</th>
		  		
		  		<th>Created At</th>
		  		<th>Updated At</th>

		  </tr>	
		</thead>
		  <% @admins.each do |admin|%>
		  <tr>
		  		<td><%=admin.id%></td>
		  		<td><%=admin.first_name%></td>
		  		<td><%= admin.last_name%></td>
		  		<td><%=admin.username %></td>
		  		
		  		<td><%=admin.created_at.strftime("%b %d %Y, %H:%M") %></td>
		  		<td><%=admin.updated_at.strftime("%b %d %Y, %H:%M") %></td>
		  		<td>
		  			
		  			<%=link_to("Edit",edit_admin_user_path(admin))%>
		  			<%=link_to("Delete",delete_admin_user_path(admin))%>
		  		</td>
		  </tr>
		  <%end%>
		</table>	
</div>
<%= will_paginate @admins %>
<footer id="footer">
<div id="footer-content foo">c.nibi123@gmail.com</div>
</footer>
</div>
